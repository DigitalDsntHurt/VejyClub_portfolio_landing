<p id="notice"><%= notice %></p>

<h1>Leads</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Fav veg</th>
      <th>Owner renter other</th>
      <th>Indoor outdoor</th>
      <th>Space</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.name %></td>
        <td><%= lead.fav_veg %></td>
        <td><%= lead.owner_renter_other %></td>
        <td><%= lead.indoor_outdoor %></td>
        <td><%= lead.space %></td>
        <td><%= lead.notes %></td>
        <td><%= link_to 'Show', lead %></td>
        <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
        <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lead', new_lead_path %>
