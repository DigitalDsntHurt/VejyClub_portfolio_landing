<p id="notice"><%= notice %></p>

<h1>Simple Leads</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @simple_leads.each do |simple_lead| %>
      <tr>
        <td><%= link_to 'Show', simple_lead %></td>
        <td><%= link_to 'Edit', edit_simple_lead_path(simple_lead) %></td>
        <td><%= link_to 'Destroy', simple_lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Simple Lead', new_simple_lead_path %>
